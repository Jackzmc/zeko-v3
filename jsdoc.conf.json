{
    "source": {
        "include": ["index.js","./src"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType" :"module",
    "recurseDepth": 4,
    "opts": {
        "destination": "./docs/",
        "recurse": true,
        "readme": "README.md",
        "template": "node_modules/minami"
    },
    "plugins": [
        "plugins/markdown"
    ]
}